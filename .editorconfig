# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# EditorConfig settings for all files
[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# EditorConfig settings for JavaScript, TypeScript, JSX, TSX files
[*.{js,jsx,ts,tsx}]
indent_style = space
indent_size = 2

